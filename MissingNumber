#include <iostream>
using namespace std;
int main() {
 int arr[]={1,3,4,5,6,7,8};
 int sumtotal=0,sum=0,n,i;
 n=sizeof(arr)/sizeof(arr[0]);
 
 sumtotal=(n+1)*(n+2)/2;
 for(i=0;i<n;i++)
 { sum=sum+arr[i];
 }
 int miss=sumtotal-sum;
 cout<<"Sum of Real Number of Series: "<<sumtotal<<endl;
cout<<"Sum of Given Series: "<<sum<<endl;
 cout<<"Missing No. is: "<<miss<<endl;
 return 0;
}
