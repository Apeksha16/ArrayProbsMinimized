//ToFindUnionofArrays
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,temp;
    int arr1[]={1,3,4,5,7};
    int arr2[]={2,3,5,6};
    int n = sizeof(arr1)/sizeof(arr1[0]);
    int m = sizeof(arr2)/sizeof(arr2[0]);
    //union
    for(i=0;i<n;i++)
    { cout<<arr1[i]<<endl;
    }
    for(j=0;j<m;j++)
    {
        temp=0;
        for(i=0;i<n;i++){
        
        if(arr1[i]==arr2[j])
        {temp=1;
        break;
        }
        }
        if(temp!=1)
        {cout<<arr2[j]<<endl;
        }
    }
    return 0;
}
