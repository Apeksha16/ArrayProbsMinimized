#include <iostream>
using namespace std;
int main()
{
    int i,j;
    int arr[] = {10, 5, 3, 4, 3, 5, 6};
    int n=sizeof(arr)/sizeof(arr[0]);
    cout<<"The Repeating Element at Smallest Index is: "<<endl;
    for(i=0;i<n;i++)
    for(j=i+1;j<n;j++)
    if(arr[i]==arr[j]){
    cout<<arr[j]<<" at index "<<i;return 0;}
    return 0;
}
