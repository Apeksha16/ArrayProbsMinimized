#include <iostream>
using namespace std;
int main() {
    int i,j,temp;
    int arr[] = {1, 2, 3, 6, 3, 6, 1};
 
    int n=sizeof(arr)/sizeof(arr[0]);
    cout<<"The Repeating Elements are: ";
    for(i=0;i<n;i++)
    {
        temp=arr[i];
        for(j=i+1;j<n;j++)
        {
        
        if (arr[i]==arr[j])
    cout<<arr[i]<<" ";
         
        }
    }
    
    
    return 0;
}
