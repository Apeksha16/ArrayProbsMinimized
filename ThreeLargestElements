#include<iostream>
using namespace std;
int main() {
    int i,j,temp;
    int arr[]={10, 4, 3, 50, 23, 90};
       int n = sizeof(arr) / sizeof(arr[0]);
       for(i=0;i<n;i++)
       {
           for(j=i+1;j<n;j++)
           {
               if(arr[i]<arr[j])
               {
                   temp=arr[i];
                   arr[i]=arr[j];
                   arr[j]=temp;
               }
           }
       }
       cout<<"Three Largest Elements in Array are: "<<endl;
       for(i=0;i<3;i++)
       {
           cout<<arr[i]<<endl;
       }
return 0;
}
