#include<iostream>

using namespace std;
int main () {
    int i;
    int sum=0;
    int sum1=0;
        int arr[] = {-2, -3, 4, -1, -2, 1, 5, -3};
    int n = sizeof(arr)/sizeof(arr[0]);
    for (i=0;i<n;i++)
    {
        sum=sum + arr[i];
        if(sum>sum1)
        sum1=sum;
        if (sum<0)
        sum=0;
        
    }
    cout<< "Maximum contiguous sum is "<<sum1;
    return 0;
}
