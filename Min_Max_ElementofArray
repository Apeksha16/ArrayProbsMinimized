#include <iostream>
using namespace std;
int main() {
int max, min, i;
int arr[]={280,96,93,360,520,66};
int n=sizeof(arr)/sizeof(arr[0]);
min=arr[0];
max=arr[1];
for (i=2;i<n;i++)
{
    if (min>arr[i])
    min=arr[i];
    else if (max<arr[i])
    max=arr[i];
}
cout<< "Minimum element of array: " <<endl;
cout<<min<<endl;
    cout << "Maximum element of array: " << endl;
    cout<<max;
}
